{
  "name": "gzhegow/error-bag",
  "type": "library",
  
  "require": {
    "php": "^7.2|^8.0",
    
    "ext-json": "*"
  },
  
  "autoload": {
    "psr-4": {
      "Gzhegow\\ErrorBag\\": "src"
    },
    "files": [
      "helpers/array.php",
      "helpers/assert.php",
      "helpers/error-bag.php"
    ]
  },

  "minimum-stability": "dev",
  "prefer-stable": true,
  
  "config": {
    "optimize-autoloader": true,
    
    "sort-packages": false
  }
}
